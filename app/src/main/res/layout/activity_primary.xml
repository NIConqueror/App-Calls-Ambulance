<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".PrimaryActivity" android:background="#8CB0D3FD" android:importantForAccessibility="auto"
>

    <ImageView
            android:layout_width="131dp"
            android:layout_height="125dp" app:srcCompat="@mipmap/ic_launcher_ambulbr"
            android:id="@+id/imageView16"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" app:layout_constraintBottom_toTopOf="@+id/textView20"/>
    <TextView
            android:text="Главная"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView20" android:textColor="#000000"
            android:textSize="36sp"
            app:layout_constraintTop_toBottomOf="@+id/imageView16" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/tabLayout"/>
    <com.google.android.material.tabs.TabLayout
            android:layout_width="370dp"
            android:layout_height="59dp"
            app:tabPaddingEnd="10dp" android:id="@+id/tabLayout"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView20"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:tabGravity="fill" app:tabMode="scrollable" android:background="#4F2547FF">

        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Главная" android:id="@+id/main"/>

        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Задать вопрос" android:id="@+id/qust"/>

        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Вызов скорой" android:id="@+id/callhosp"/>
        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/profile" android:text="Профиль"/>
    </com.google.android.material.tabs.TabLayout>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="339dp"
            android:layout_height="84dp"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar2"
            app:navigationIcon="@android:drawable/ic_dialog_info"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/toolbar4"
            app:collapseIcon="@android:drawable/ic_dialog_info"/>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="339dp"
            android:layout_height="84dp"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar4"
            app:navigationIcon="@android:drawable/stat_sys_warning"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            android:layout_marginTop="11dp" app:layout_constraintTop_toBottomOf="@+id/toolbar3"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/toolbar2"/>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="339dp"
            android:layout_height="84dp"
            android:background="?attr/colorPrimary"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar3"
            app:navigationIcon="@android:drawable/stat_sys_phone_call"
            app:layout_constraintTop_toBottomOf="@+id/tabLayout" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintBottom_toTopOf="@+id/toolbar4"
            app:collapseIcon="@drawable/common_full_open_on_phone" android:layout_marginTop="8dp"/>
    <TextView
            android:text="Вызовы поступающие на станцию скорой медецинской помощи по единому номеру 103 либо по другому номеру телефона,  согласованному .."
            android:layout_width="266dp"
            android:layout_height="40dp"
            android:id="@+id/textView21"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="36dp" android:layout_marginRight="36dp"
            android:textColor="#FFFFFF" android:textSize="11dp" app:layout_constraintTop_toBottomOf="@+id/textView22"
    />
    <TextView
            android:text="Вызов скорой и все возможные способы"
            android:layout_width="267dp"
            android:layout_height="30dp"
            android:id="@+id/textView22"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="36dp" android:layout_marginRight="36dp"
            app:layout_constraintTop_toBottomOf="@+id/tabLayout"
            app:layout_constraintBottom_toTopOf="@+id/toolbar3" android:layout_marginTop="32dp"
            app:layout_constraintVertical_bias="0.309" android:textColor="#FFFFFF"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@mipmap/ic_launcherlik3_round"
            android:id="@+id/imageView17"
            android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/tabLayout"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="300dp"
            android:layout_marginRight="300dp"/>
    <TextView
            android:text="Как защититься от коронавируса?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView25"
            android:layout_marginBottom="64dp" app:layout_constraintBottom_toTopOf="@+id/toolbar2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.596"
            app:layout_constraintEnd_toEndOf="parent" android:textColor="#FFFFFF"/>
    <TextView
            android:text="Первый случай коронавируса потверждён в Беларуси, в связи с чем министр здравохранения страны ..."
            android:layout_width="261dp"
            android:layout_height="52dp"
            android:id="@+id/textView27" app:layout_constraintStart_toEndOf="@+id/toolbar4"
            app:layout_constraintHorizontal_bias="0.446" app:layout_constraintEnd_toStartOf="@+id/toolbar4"
            android:textColor="#FFFFFF" android:textSize="11sp" app:layout_constraintTop_toBottomOf="@+id/textView25"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@mipmap/ic_launcherlik_round"
            android:id="@+id/imageView21"
            app:layout_constraintStart_toEndOf="@+id/toolbar4" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintEnd_toStartOf="@+id/textView27" app:layout_constraintTop_toBottomOf="@+id/toolbar4"
            app:layout_constraintBottom_toTopOf="@+id/toolbar4" app:layout_constraintVertical_bias="0.425"
            android:layout_marginRight="22dp" android:layout_marginEnd="22dp" android:layout_marginTop="12dp"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
    <TextView
            android:text="Новости дня :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView28" android:textColor="#FFFFFF" app:layout_constraintStart_toEndOf="@+id/toolbar2"
            app:layout_constraintHorizontal_bias="0.619" app:layout_constraintEnd_toStartOf="@+id/toolbar2"
            app:layout_constraintBottom_toTopOf="@+id/textView29" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="485dp"/>
    <TextView
            android:text="Бригада скорой помощи должна прибыть к месту оказания помощи не более чем 20 минут"
            android:layout_width="261dp"
            android:layout_height="51dp"
            android:id="@+id/textView29" android:textColor="#FFFFFF"
            android:textSize="11sp" app:layout_constraintTop_toBottomOf="@+id/textView28"
            app:layout_constraintStart_toEndOf="@+id/toolbar2" app:layout_constraintHorizontal_bias="0.445"
            app:layout_constraintEnd_toStartOf="@+id/toolbar2" android:layout_marginTop="4dp"/>
    <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp" app:srcCompat="@mipmap/ic_launcherlik3"
            android:id="@+id/imageView22"
            app:layout_constraintStart_toEndOf="@+id/toolbar2" app:layout_constraintHorizontal_bias="0.868"
            app:layout_constraintEnd_toStartOf="@+id/toolbar2" app:layout_constraintTop_toBottomOf="@+id/toolbar2"
            app:layout_constraintBottom_toTopOf="@+id/toolbar2" app:layout_constraintVertical_bias="0.322"/>
    <androidx.constraintlayout.widget.Barrier android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/barrier2" app:barrierDirection="left"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Barrier android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/barrier3" app:barrierDirection="left"/>
</androidx.constraintlayout.widget.ConstraintLayout>