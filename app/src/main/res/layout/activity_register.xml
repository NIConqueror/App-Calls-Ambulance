            android:text='(Нажмите на кнопку "Фото"'
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11" android:textColor="#A6615C5C"
            android:textSize="12sp" app:layout_constraintBottom_toTopOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/button4" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="чтобы прикрепить изображение)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" android:textColor="#A6615C5C"
            android:textSize="12sp" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintBottom_toTopOf="@+id/button5"/>
    <Button
            android:text="Зарегестрироваться"
            android:layout_width="215dp"
            android:layout_height="26dp"
            android:id="@+id/button5" android:background="#398EFE"
            android:textColor="#FFFFFF" app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"
    />
    <ImageView
            android:layout_width="50dp"
            android:layout_height="35dp" app:srcCompat="@drawable/ic_launcher_ur"
            android:id="@+id/imageView9"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toStartOf="@+id/editText3"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp" android:layout_marginTop="35dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            app:layout_constraintBottom_toTopOf="@+id/imageView10"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="35dp" app:srcCompat="@drawable/ic_launcher_ur"
            android:id="@+id/imageView10"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toStartOf="@+id/editText7"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView9" android:layout_marginBottom="3dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView11"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="35dp" app:srcCompat="@drawable/ic_launcher_call"
            android:id="@+id/imageView11"
            app:layout_constraintEnd_toStartOf="@+id/editText8" android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView10"
            app:layout_constraintBottom_toTopOf="@+id/imageView12" android:layout_marginTop="3dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="35dp" app:srcCompat="@drawable/ic_launcher_key"
            android:id="@+id/imageView12"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toStartOf="@+id/editText8"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView11"
            app:layout_constraintBottom_toTopOf="@+id/imageView13"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="30dp" app:srcCompat="@drawable/ic_launcher_of"
            android:id="@+id/imageView13"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toStartOf="@+id/editText9"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView12"
            app:layout_constraintBottom_toTopOf="@+id/imageView15"
    />
    <ImageView
            android:layout_width="50dp"
            android:layout_height="30dp" app:srcCompat="@drawable/ic_launcher_k"
            android:id="@+id/imageView14"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toStartOf="@+id/editText10"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintHorizontal_bias="0.428"
            app:layout_constraintTop_toBottomOf="@+id/imageView15" android:layout_marginTop="5dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="30dp" app:srcCompat="@drawable/ic_launcher_k"
            android:id="@+id/imageView15"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toStartOf="@+id/editText10"
            android:layout_marginEnd="15dp" android:layout_marginRight="15dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView13"
            app:layout_constraintHorizontal_bias="0.428"
            app:layout_constraintBottom_toTopOf="@+id/imageView14" android:layout_marginTop="2dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>