<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RegisterActivity" android:background="#B0D3FD">

    <ImageView
            android:layout_width="135dp"
            android:layout_height="121dp" app:srcCompat="@mipmap/ic_launcher_ambulbr"
            android:id="@+id/imageView7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="2dp"/>
    <TextView
            android:text="Вызов скорой помощи"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" android:layout_marginTop="2dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" android:textSize="28sp"
            android:textColor="@android:color/holo_red_dark" android:fontFamily="c"
            app:layout_constraintBottom_toTopOf="@+id/textView10"/>
    <TextView
            android:text="103"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:id="@+id/textView10" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" android:textSize="30sp"
            android:textColor="@android:color/holo_red_dark" android:fontFamily="@font/noto_sans_bold"
            app:layout_constraintBottom_toTopOf="@+id/editText3"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="textPersonName"
            android:text="ФИО"
            android:ems="16"
            android:id="@+id/editText3"
            android:textSize="12sp" android:textColor="#80000000" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintBottom_toTopOf="@+id/editText7"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="textPersonName"
            android:text="Пациент, врач (выбрать)"
            android:ems="16"
            android:id="@+id/editText7"
            android:textSize="12sp" android:textColor="#80000000" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/editText3"
            app:layout_constraintBottom_toTopOf="@+id/editText8"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="phone"
            android:ems="16"
            android:id="@+id/editText8"
            android:text="Мобильный номер телефона" android:textSize="12sp" android:textColor="#80000000"
            app:layout_constraintTop_toBottomOf="@+id/editText7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"
            app:layout_constraintBottom_toTopOf="@+id/editText9" android:layout_marginBottom="32dp"
    />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="textPersonName"
            android:text="Место проживания, адрес"
            android:ems="16"
            android:id="@+id/editText9"
            android:textSize="12sp" android:textColor="#80000000" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            app:layout_constraintTop_toBottomOf="@+id/editText8"
    />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="textPostalAddress"
            android:ems="16"
            android:id="@+id/editText10"
            android:text="Номер больницы (привязка)" android:textSize="12sp" android:textColor="#80000000"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintTop_toBottomOf="@+id/editText9" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="textPassword"
            android:ems="16"
            android:id="@+id/editText11"
            android:text="Придумайте себе пароль" android:textSize="12sp" android:textColor="#80000000"
            app:layout_constraintTop_toBottomOf="@+id/editText10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            android:layout_marginRight="32dp"
            android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:inputType="textPassword"
            android:ems="16"
            android:id="@+id/editText13"
            android:text="Придумайте себе пароль" android:textSize="12sp" android:textColor="#80000000"
            app:layout_constraintTop_toBottomOf="@+id/editText11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"/>
    <Button
            android:text="Фото"
            android:layout_width="94dp"
            android:layout_height="23dp"
            android:id="@+id/button4" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/editText13" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" android:textColor="#FFFFFF" android:background="#398EFE"/>
